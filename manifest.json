{
  "manifest_version": 2,

  "name": "YGrab!",
  "description": "Find images and upload it to flickr",
  "version": "1.0",

  "permissions": [
    "https://secure.flickr.com/", "http://*/*", "https://*/*"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
  		{ 
  			"js" : 		[ "jquery.min.js","run.js"],	 
  			"matches" : [ "http://*/*", "https://*/*" ]
  		}	
   ]
}